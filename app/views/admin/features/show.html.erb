<ul class='list-group mt-4'>
  <li class='list-group-item'>
    <strong class='text-primary'>Feature Name: </strong> <%= @feature.feature_name %>
  </li>
  <li class='list-group-item'>
    <strong class='text-primary'>Code: </strong> <%= @feature.code %>
  </li>
  <li class='list-group-item'>
    <strong class='text-primary'>Unit Price:</strong> <%= @feature.unit_price %>$
  </li>
  <li class='list-group-item'>
    <strong class='text-primary'>Max Unit Limit:</strong> <%= @feature.max_unit_limit %>
  </li>
  <li class='list-group-item'>
    <div class='btn-group mr-2' role='group' aria-label='links'>
      <%= link_to 'Edit', edit_admin_feature_path(@feature), class:'btn btn-primary' %>
      <%= link_to 'Back', admin_features_path, class:'btn btn-info' %>
    </div>
  </li>
</ul>
